  "dependencies": {
    "@fastify/static": "6.12.0",
    "@langchain/core": "0.1.24",
    "@langchain/openai": "0.0.14",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "3.2.0",
    "@nestjs/core": "^10.3.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "10.3.2",
    "@nestjs/swagger": "7.2.0",
    "@nestjs/typeorm": "10.0.1",
    "@tsconfig/node18": "18.2.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "http-status-codes": "2.3.0",
    "joi": "17.12.1",
    "langchain": "0.1.14",
    "npm": "10.4.0",
    "pg": "8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "0.3.20",
    "typeorm-naming-strategies": "4.1.0",
    "uuid": "9.0.1",
    "yaml": "2.3.4",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "10.3.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "9.0.10",
    "jest": "^29.5.0",
    "lint-staged": "15.2.2",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix"
    ],
    "*.js": "eslint --cache --fix"
  },
  "private": true,
  "scripts": {
    "build": "nest build",
    "build:watch": "npx nest build --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,e2e}/**/*.ts\" --fix",
    "prepare": "husky",
    "start": "nest start",
    "start:debug": "nest start --watch --config nest-cli.json --debug 0.0.0.0:9234",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./src/e2e/jest-e2e.js --runInBand",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "typeorm": "typeorm -d dist/ormconfig.js"
  }
}
